
@{
    ViewBag.Title = "AddDesignation";
    Layout = "~/Views/Shared/Dashboard.cshtml";
}


<link href="~/Table/css/style.css" rel="stylesheet" />



<br />
<div class="row clearfix">
    <div class="col-md-12">
        <div class="card" style="height:160%; ">
            <div class="header">
                <h5>AddDesignation</h5>
            </div>
            <div class="body">
                
                <div class="row">
                    <div class="col-md-6">
                        <form id="basic-form" method="post" novalidate>
                            <div class="form-group">
                                <label>ID</label>
                                <input type="text" name="Name" id="D_Id" readonly class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label>Designation</label>
                                <input type="text" name="Name" id="D_Name" class="form-control" required>
                            </div>



                            <button type="button" class="btn  btn-primary btn-block col-md-12" onclick="AddDesignation()">Save</button>
                        </form>
                    </div>

                    <div class="col-md-6" style="margin-top:-50px">
                        <section class="col-md-10 table-bordered table-striped table-hover table-responsive-sm " style="margin-left:50px;">




                            <h1>Classes List</h1>

                            <div class="tbl-content">
                                <table cellpadding="0" cellspacing="0" border="1">
                                    <thead class="thead-dark">
                                        <tr>
                                            @*<th class="table-dark">Id</th>*@
                                            <th class="table-dark">Name</th>

                                            <th class="table-dark">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbllist"></tbody>
                                </table>
                            </div>
                        </section>



                    </div>

                </div>


            </div>
        </div>
    </div>

</div>


<script src="~/asset/js/jquery.min.js"></script>
<script src="~/jquery/Designation.js"></script>
<script src="~/Table/js/index.js"></script>
<script src="~/asset/Common.js"></script>